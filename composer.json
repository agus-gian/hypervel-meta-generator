{
    "name": "augustpermana/hypervel-meta-generator",
    "description": "A Hypervel package to generate and manage metadata for models",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "hypervel-package",
    "license": "MIT",
    "keywords": [
        "hypervel",
        "meta",
        "meta-table",
        "meta-generator",
        "meta-table-generator",
        "swoole",
        "coroutine"
    ],
    "authors": [
        {
            "name": "Agus Permana",
            "email": "agus.emailnya@gmail.com"
        }
    ],
    "homepage": "https://github.com/agus-gian/hypervel-meta-generator",
    "require": {
        "php": ">=8.2",
        "hypervel/framework": "^0.3",
        "ext-json": "*",
        "ext-swoole": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "AugustPermana\\HypervelMetaGenerator\\": "src/"
        }
    },
    "extra": {
        "hypervel": {
            "providers": [
                "AugustPermana\\HypervelMetaGenerator\\ServiceProvider"
            ]
        }
    }
}
